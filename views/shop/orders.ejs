<%- include('../includes/head.ejs') %>

<body>
<%- include('../includes/navigation.ejs') %>
<main>
	<% if (orders.length <= 0) { %>
	<h1>Nothing there!</h1>
	<% } else { %>
		<ul>
			<% orders.forEach(order => {%>
			<li>
				<h2># <%= order._id%></h2>
				<ul>
					<% order.items.forEach(product => { %>
					<li><%= product.title %> (<%= product.quantity %>)</li>
					<%}) %>
				</ul>
			</li>
			<%}) %>
		</ul>
		<% } %>
</main>
<%- include('../includes/end.ejs') %>